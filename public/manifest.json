{
  "name": "Breathe Focus",
  "version": "1.0",
  "description": "Timed focus sessions will eliminate noise and calm you down",
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "index.html",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
